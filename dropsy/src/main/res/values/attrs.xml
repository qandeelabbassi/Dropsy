<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Dropsy">
        <attr name="dropsyFontType" format="string">
            <enum name="roboto_regular" value="0" />
            <enum name="roboto_thin" value="1" />
            <enum name="roboto_light" value="2" />
            <enum name="roboto_medium" value="3" />
            <enum name="roboto_bold" value="4" />
            <enum name="roboto_black" value="5" />
        </attr>

        <attr name="dropsyItems" format="integer"/>
        <attr name="dropsyElevation" format="dimension"/>
        <attr name="dropsyLabel" format="string"/>
    </declare-styleable>
</resources>